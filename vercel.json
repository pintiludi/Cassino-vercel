{
  "version": 2,
  "builds": [
    { "src": "api/gerar-pagamento.js", "use": "@vercel/node" },
    { "src": "api/webhook.js", "use": "@vercel/node" },
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/gerar-pagamento", "dest": "/api/gerar-pagamento.js" },
    { "src": "/api/webhook", "dest": "/api/webhook.js" },
    { "src": "/", "dest": "/index.js" }
  ]
}
